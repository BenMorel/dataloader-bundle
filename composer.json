{
    "name": "overblog/dataloader-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "DataLoader Symfony bundle implementation.",
    "keywords": ["dataLoader", "caching", "batching"],
    "authors": [
        {
            "name": "Overblog",
            "homepage": "http://www.over-blog.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Overblog\\DataLoaderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Overblog\\DataLoaderBundle\\Tests\\": "tests/"
        }
    },
    "config" : {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": "^8.0",
        "overblog/dataloader-php": "^0.7.0",
        "symfony/dependency-injection": "^5.3 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "react/promise": "^2.5",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/yaml": "^5.3 || ^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
