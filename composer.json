{
    "name": "overblog/dataloader-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "DataLoader Symfony bundle implementation.",
    "keywords": ["dataLoader", "caching", "batching"],
    "authors": [
        {
            "name": "Overblog",
            "homepage": "http://www.over-blog.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Overblog\\DataLoaderBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config" : {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": ">=5.5",
        "overblog/dataloader-php": "^0.5.0",
        "symfony/dependency-injection": "^2.7 || ^3.1 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1 || ^5.5 || ^6.0",
        "react/promise": "^2.5",
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/phpunit-bridge": "^2.7 || ^3.1 || ^4.0",
        "symfony/yaml": "^2.7 || ^3.1 || ^4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
